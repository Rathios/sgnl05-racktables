{
  "name": "sgnl05-racktables",
  "version": "0.1.2",
  "author": "sgnl05",
  "summary": "Puppet module for pulling the latest development version of Racktables",
  "license": "Apache 2.0",
  "source": "git://github.com/sgnl05/sgnl05-racktables.git",
  "project_page": "https://github.com/sgnl05/sgnl05-racktables",
  "issues_url": "https://github.com/sgnl05/sgnl05-racktables/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6"
      ]
    },
   {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7"
      ]
    },
   {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04"
      ]
    }
 ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0 < 3.4.0"
    },
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 2.4.0"
    },
    {
      "name": "puppetlabs/apache",
      "version_requirement": ">= 1.1.1"
    },
    {
      "name": "puppetlabs/mysql",
      "version_requirement": ">= 2.3.1"
    },
    {
      "name": "puppetlabs/vcsrepo",
      "version_requirement": ">= 1.1.0"
    }
  ]
}
